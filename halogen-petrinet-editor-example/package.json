{
  "name": "purescript-halogen-petrinet-editor-example",
  "version": "1.0.0",
  "description": "Petri net editor example in PureScript Halogen",
  "main": "index.js",
  "scripts": {
    "postinstall": "spago install",
    "build": "spago build -- --censor-codes=ImplicitImport,ImplicitQualifiedImport,HidingImport",
    "bundle": "spago bundle-app --main Main --to dist/app.js -- --censor-codes=ImplicitImport,ImplicitQualifiedImport,HidingImport",
    "watch": "spago bundle-app --watch --main Main --to dist/app.js -- --censor-codes=ImplicitImport,ImplicitQualifiedImport,HidingImport",
    "test": "spago test"
  },
  "keywords": [
    "purescript",
    "halogen",
    "petrinet",
    "editor",
    "example"
  ],
  "author": "Erik Post <erik@shinsetsu.nl>",
  "license": "ISC",
  "devDependencies": {
    "purescript": "^0.13.3",
    "purescript-psa": "^0.7.3",
    "spago": "^0.9.0"
  }
}
