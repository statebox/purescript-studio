{
  "name": "stbx-service-rest",
  "version": "1.0.0",
  "description": "Statebox REST API",
  "main": "index.js",
  "scripts": {
    "postinstall": "spago install",
    "init": "spago init",
    "build": "spago build",
    "repl": "spago repl",
    "docs": "spago docs",
    "watch": "spago build --watch",
    "serve": "node index.js",
    "test": "npm-run-all --print-label --race --parallel 'test:serve' 'test:waitThenRequests'",
    "test:serve": "node index.js",
    "test:requests": "sh scripts/test.sh",
    "test:waitThenRequests": "npm run wait && npm run test:requests",
    "wait": "sleep 1"
  },
  "author": "Erik Post <erik@shinsetsu.nl>",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "purescript": "^0.13.3",
    "spago": "^0.9.0"
  },
  "dependencies": {
    "@statebox/stbx-js": "0.0.31",
    "body-parser": "^1.19.0",
    "express": "^4.17.1"
  }
}
