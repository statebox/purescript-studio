{
  "name": "statebox-studio",
  "version": "1.0.0",
  "description": "Statebox Studio",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "postinstall": "psc-package install",
    "build": "pulp --psc-package build",
    "watch": "pulp --psc-package -w browserify --to dist/app.js",
    "bundle": "pulp --psc-package browserify --optimise --to dist/app.js",
    "test": "pulp --psc-package test",
    "repl": "pulp repl",
    "bower-build": "pulp run",
    "bower-test": "pulp test"
  },
  "keywords": [
    "statebox",
    "petrinet"
  ],
  "author": "Erik Post <erik@shinsetsu.nl>",
  "license": "Commercial",
  "devDependencies": {
    "pulp": "^12.3.1",
    "purescript": "^0.12.2",
    "purescript-psa": "^0.7.3"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.6.3",
    "psc-package": "^3.0.1",
    "@statebox/stbx-js": "0.0.21",
    "tailwindcss": "^0.7.3"
  }
}
